    macros {
        ppt_fp: ppt_fp {
            compatible = "zmk,behavior-macro";
            label = "PPT format painter";
            #binding-cells = <0>;
            bindings = <&kp LALT &kp H &kp F &kp P>;
        };
        wink: wink {
            compatible = "zmk,behavior-macro";
            label = "^^";
            #binding-cells = <0>;
            bindings = <&kp CARET &kp CARET>;
        };
        mak_ae: mak_ae {
            compatible = "zmk,behavior-macro-one-param";
            label = "mak_ae";
            wait-ms = <0>;
            tap-ms = <0>;
            #binding-cells = <1>;
            bindings =
                <WIN_U_LEAD>,
                <&macro_param_1to1>,
                <&macro_tap MACRO_PLACEHOLDER>,
                <WIN_U_TRAIL>;
        };
        mak_ae_G: mak_ae_G {
            compatible = "zmk,behavior-macro";
            label = "mak_ae_G";
            wait-ms = <0>;
            tap-ms = <0>;
            #binding-cells = <0>;
            bindings =
                <WIN_U_LEAD>,
                <&macro_tap &kp N0 &kp N0 &kp C &kp N4>,
                <WIN_U_TRAIL>;
        };
    };